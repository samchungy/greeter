{
  "name": "@seek/greeter",
  "private": true,
  "license": "UNLICENSED",
  "imports": {
    "#src/*": {
      "@seek/greeter/source": "./src/*",
      "default": "./lib/*"
    }
  },
  "scripts": {
    "build": "skuba build",
    "format": "skuba format",
    "lint": "skuba lint",
    "start": "skuba start",
    "start:debug": "pnpm --silent start --inspect-brk",
    "test": "skuba test",
    "test:ci": "skuba test --coverage",
    "test:watch": "skuba test --watch"
  },
  "devDependencies": {
    "@types/node": "22.13.4",
    "skuba": "14.1.1"
  },
  "packageManager": "pnpm@10.8.1",
  "engines": {
    "node": ">=24"
  },
  "skuba": {
    "entryPoint": "src/app.ts",
    "template": "greeter",
    "type": "application",
    "version": "14.1.1"
  }
}
